<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="AllMarketPlaceTestSuite" verbose="1">
    <parameter name="team" value="SCM"/>
	<parameter name="core" value="Marketplace"/>
	<parameter name="type" value="Regression"/>
	<listeners>
		<listener class-name="com.myntra.lordoftherings.SCMTestListener" />
	</listeners>
	<test name="AllMarketPlaceTests">
		<groups>
			<run>
				<include name="Regression" />
			</run>
		</groups>
		
		<classes>
            <class name="com.myntra.apiTests.erpservices.marketplace.JITOrderPlacementTest" />
            <class name="com.myntra.apiTests.test.regression.marketplace.PartnerPortalSeller" />
            <class name="com.myntra.apiTests.erpservices.marketplace.RmsGatePassEndToEnd" />
            <class name="com.myntra.apiTests.erpservices.marketplace.RtoGatePassEndToEnd" />
            <class name="com.myntra.apiTests.erpservices.sellerapis.Tests.SellerServiceTest" />
           
		</classes>
	</test>
</suite>